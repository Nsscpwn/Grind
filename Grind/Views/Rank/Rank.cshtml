@model  Grind.Models.Main
@{
    ViewBag.Title = "Rank";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
<div class="col-md-4">
    @foreach (var r in Model.SQL.totalplayers())
    {
        <div class="row">
            @r.nrplayers
        </div>
    }
</div>
<div class="col-md-4">
    @foreach (var r in Model.SQL.getRankNames())
    {
        <div class="row">
            @r.name
        </div>
    }
</div>
<div class="col-md-4">
    @foreach (var r in Model.SQL.getRankLvl())
    {
        <div class="row">
            @r.lvl
        </div>
    }
</div>
